#!name = Tự động tham gia TestFlight
#!desc = Tự động giám sát danh sách chỗ trống trong TestFlight và tham gia ngay khi có chỗ trống mới.
#!author = YuiChy
#!icon = https://gitlab.com/lodepuly/iconlibrary/-/raw/main/App_icon/120px/TestFlight.png
#!input = APP_ID
#!date = 2024-04-29 20:10:00

[Rule]
DOMAIN, testflight.apple.com, PROXY

[Script]
http-request ^https://testflight.apple.com/join/(.*) script-path = https://raw.githubusercontent.com/YuiChy-CSA/TF/main/TF_keys2.js, tag = Lấy APP_ID
http-request ^https?:\/\/testflight\.apple\.com\/v3\/accounts\/.*\/apps$ script-path = https://raw.githubusercontent.com/YuiChy-CSA/TF/main/TF_keys2.js, tag = Lấy thông tin TF
cron "*/1 * * * * *" script-path = https://raw.githubusercontent.com/YuiChy-CSA/TF/main/Auto_join_TF2.js, tag = Tự động tham gia TF, timeout = 180

[Mitm]
hostname = testflight.apple.com